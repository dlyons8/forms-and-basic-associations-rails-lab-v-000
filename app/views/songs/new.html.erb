<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :genre %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <br />
  <%= f.text_field :artist_name, list: "categories_autocomplete" %>
 <datalist id="categories_autocomplete">
   <% Artist.all.each do |artist| %>
     <option value="<%= artist.name %>">
   <% end %>
 </datalist>
 <br />

  <%= f.submit %>
<% end %>
